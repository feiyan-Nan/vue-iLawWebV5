{
  "name": "iLawWeb2.0",
  "version": "1.8.0",
  "description": "Alpha 前端Vue版本",
  "private": true,
  "scripts": {
    "dev": "export NODE_ENV=dev && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:test": "export NODE_ENV=test && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:prod": "export NODE_ENV=production && webpack-dev-server --inline --progress --config build/webpack.prod.conf.js",
    "dev:pre": "export NODE_ENV=pre && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "node build/build.js",
    "commit": "git-cz",
    "changelog": "standard-version"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "solarlunar": "^2.0.2",
    "vue": "^2.5.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
